# Server Configuration
NODE_ENV=development
PORT=3001

# CORS Configuration
FRONTEND_URL=http://localhost:5173

# Logging
LOG_LEVEL=info

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Rate Limiting
RATE_LIMIT_WINDOW_MS=1000
RATE_LIMIT_MAX_REQUESTS=5

# WebRTC SFU Configuration (Phase 4)
# Provider options: "mock" (development), "livekit" (cloud), "mediasoup" (self-hosted)
SFU_PROVIDER=mock
SFU_ROOM_TTL=0

# LiveKit Configuration (only needed if SFU_PROVIDER=livekit)
# Get these from https://cloud.livekit.io
# LIVEKIT_URL=wss://your-workspace.livekit.cloud
# LIVEKIT_API_KEY=your-api-key
# LIVEKIT_API_SECRET=your-api-secret
